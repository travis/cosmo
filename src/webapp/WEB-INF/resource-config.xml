<?xml version="1.0" encoding="UTF-8"?>
<config>
  <iomanager>
    <class name="org.osaf.cosmo.dav.jcr.io.JcrDavIOManager"/>
  </iomanager>
  <collection>
    <nodetypes>
      <nodetype>nt:folder</nodetype>
    </nodetypes>
  </collection>
  <filter>
    <class name="org.apache.jackrabbit.webdav.simple.DefaultItemFilter"/>
    <nodetypes>
      <nodetype>nt:base</nodetype>
    </nodetypes>
    <namespaces>
      <prefix>rep</prefix>
      <prefix>jcr</prefix>
      <prefix>icalendar</prefix>
      <prefix>calendar</prefix>
      <prefix>ticket</prefix>
      <prefix>xml</prefix>
      <prefix>cosmo</prefix>
      <prefix>dav</prefix>
    </namespaces>
  </filter>
</config>
