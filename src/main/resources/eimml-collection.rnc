# -*- rnc -*-
# RELAX NG Compact Syntax Grammar for the
# EIMML collection record type Version 1

namespace collection = "http://osafoundation.org/eim/collection"

include "eimml-core.rnc"

eimCollectionRecord =
   element collection:record {
      (eimDeleted,
         collectionUuid
       & anyCollectionField*)
   }

collectionUuid =
   element collection:uuid {
      eimTextFieldContent
   }

anyCollectionField =
   element collection:* {
      (eimTextFieldContent
       | eimDecimalFieldContent
       | eimIntegerFieldContent
       | eimDateTimeFieldContent
       | eimTimeStampFieldContent
       | eimBytesFieldContent
       | eimBlobFieldContent
       | eimClobFieldContent)*
   }
